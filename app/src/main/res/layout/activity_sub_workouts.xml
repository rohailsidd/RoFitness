<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#376287">

    <include
        android:id="@+id/layout_sub_workout_header"
        layout="@layout/layout_workout_header" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView_subWorkouts_subWorkoutsList"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/layout_sub_workout_header" />
</androidx.constraintlayout.widget.ConstraintLayout>